{
    "name": "cric-ui",
    "version": "1.0.6",
    "private": false,
    "author": "seven.zhang@foxmail.com",
    "description": "CricUi - A Vue.js 3 UI library",
    "keywords": [
        "CricUi"
    ],
    "homepage": "https://seven777777.github.io/cric-ui/",
    "repository": {
        "type": "git",
        "url": "https://github.com/seven777777/cric-ui"
    },
    "files": [
        "lib/*",
        "packages/*",
        "README.md",
        "package.json"
    ],
    "main": "./lib/cric-ui.umd.js",
    "module": "./lib/cric-ui.mjs",
    "exports": {
        ".": {
            "import": "./lib/cric-ui.mjs",
            "require": "./lib/cric-ui.umd.js"
        },
        "./lib/style.css": "./lib/style.css"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,vue}": [
            "prettier --write",
            "eslint --cache --fix",
            "git add"
        ]
    },
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write .",
        "prepare": "husky install",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "precommit": "eslint src/**/*.ts"
    },
    "dependencies": {
        "@vitepress-demo-preview/component": "^2.3.0",
        "@vitepress-demo-preview/plugin": "^1.1.9",
        "cric-ui": "^1.0.6",
        "element-plus": "^2.3.5",
        "husky": "^8.0.3",
        "sass": "^1.62.1",
        "vite-plugin-vue-setup-extend": "^0.4.0",
        "vitepress": "^1.0.0-alpha.75",
        "vue": "^3.3.2"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.2.0",
        "@tsconfig/node18": "^2.0.1",
        "@types/jsdom": "^21.1.1",
        "@types/node": "^18.16.8",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.3.2",
        "@vue/tsconfig": "^0.4.0",
        "eslint": "^8.39.0",
        "eslint-plugin-vue": "^9.11.0",
        "jsdom": "^22.0.0",
        "lint-staged": "^13.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.8",
        "typescript": "~5.0.4",
        "vite": "^4.3.5",
        "vitest": "^0.31.0",
        "vue-tsc": "^1.6.4"
    }
}
